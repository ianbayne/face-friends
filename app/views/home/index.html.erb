<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% @posts.each do |post| %>
  <div>
    <p><%= post.created_at %></p>
    <%= image_tag post.image_url unless post.image_data.nil? %>
    <p><%= post.body %></p>
  </div>
<% end %>

<%= link_to('Logout', destroy_user_session_path, method: :delete) %>
